
public class FlujoSwitch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*Switch*/
		int dia = 1;
		String mensaje = "";
		switch(dia) {
			case 1:
				mensaje = "Lunes";
				break;
			case 2:
				mensaje = "Martes";
				break;
			case 3:
				mensaje = "Miercoles";
				break;
			case 4:
				mensaje = "Jueves";
				break;
			case 5:
				mensaje = "Viernes";
				break;
		}
	}

}
